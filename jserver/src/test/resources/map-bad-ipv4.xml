<edu.rutgers.winlab.mfirst.mapping.ipv4udp.Configuration>
    <!-- Prefix filename. Maps network address prefixes to AS numbers. Will be
       removed in future versions. -->
  <prefixFile>src/test/resources/prefixes.ipv4</prefixFile>

  <!-- AS bindings filename. Maps Autonomous Systems to their network
       identities (IP address, port). Will be removed in future versions. -->
  <asBindingFile>src/test/resources/as-binding.ipv4</asBindingFile>
    
  <!-- The hashing algorithm to use.  Should be one of: MD2, MD5, SHA-1,
       SHA-256, SHA-384, or SHA-512. -->
  <hashAlgorithm>foobar</hashAlgorithm>
    
</edu.rutgers.winlab.mfirst.mapping.ipv4udp.Configuration>
